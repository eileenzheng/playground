<?xml version="1.0" encoding="UTF-8"?>
<suite name="UCHC Production Regression" verbose="5" parallel="classes" thread-count="6">
    <parameter name="url" value="http://uchc:maggiegetyourbone@stagea.ucomparehealthcare.com" />
    <parameter name="browser" value="firefox" />
    <parameter name="driverType" value="remote" />

    <test name="Browse Path Tests">
        <classes>
            <class name="com.uchc.test.BrowsePathTest"/>
        </classes>
    </test>
    <test name="Search Specfic Tests">
        <classes>
            <class name="com.uchc.test.SearchTest"/>
            <methods>
                <exclude name="byDoctorPageRanges" />
                <exclude name="byDoctorPagination" />
                <exclude name="bySpecialtyPageRanges" />
                <exclude name="bySpecialtyPagination" />
                <exclude name="byNamePageRanges" />
                <exclude name="byNamePagination" />
                <exclude name="byDentistPageRanges" />
                <exclude name="byDentistPagination" />
                <exclude name="byPodiatristPageRanges" />
                <exclude name="byPodiatristPagination" />
            </methods>
        </classes>
    </test>
    <test name="Profile Specfic Tests">
        <classes>
            <class name="com.uchc.test.ProfileTest"/>
        </classes>
    </test>
    <test name="Patient Link Tests">
        <classes>
            <class name="com.uchc.test.PatientLinkTest"/>
        </classes>
    </test>
    <test name="DFP Tests">
        <classes>
            <class name="com.uchc.test.DFPTest"/>
        </classes>
    </test>
</suite>