<?xml version="1.0" encoding="UTF-8"?>
<suite name="Vitals Staging Regression" verbose="5" parallel="classes" thread-count="2">
    <parameter name="url" value="http://staging.vitals.com"/>
    <parameter name="browser" value="firefox" />
	<parameter name="projectId" value="2"/>
	<parameter name="suiteId" value="12"/>
	<parameter name="driverType" value="remote" />

	<listeners>
		<listener class-name="com.vitals.WebDriverListener" />
		<!-- <listener class-name="com.vitals.TestRailResultListener" /> -->
	</listeners>

	<test name="Profile Specific Tests">
		<classes>
			<class name="com.vitals.test.ProfileTest" />
		</classes>
	</test>
	<test name="Search Specfic Tests">
		<classes>
			<class name="com.vitals.test.SearchTest" />
			<methods>
				<exclude name="compareResultsToProfile" />
			</methods>

		</classes>
	</test>
	<test name="PatientGuide Specific Tests">
		<classes>
			<class name="com.vitals.test.PatientGuideTest" />
		</classes>
	</test>
	<test name="Patientlink Specific Tests">
		<classes>
			<class name="com.vitals.test.PatientLinkTest" />
		</classes>
	</test>
	<test name="MyVitals Specific Tests">
		<classes>
			<class name="com.vitals.test.MyVitalsTest" />
		</classes>
	</test> 
	<test name="Masthead Specific Tests">
		<classes>
			<class name="com.vitals.test.MastheadTest" />
		</classes>
	</test>
 	<test name="Urgent Care Specific Tests">
		<classes>
			<class name="com.vitals.test.UccTest" />
		</classes>
	</test> 
</suite>